{
    "name": "clayliddell/shoppingcart",
    "description": "Shopping Cart implementation for Laravel 5 & 6.",
    "license": "MIT",
    "type": "laravel",
    "minimum-stability": "dev",
    "keywords": [
        "laravel",
        "eloquent",
        "model",
        "shopping cart"
    ],
    "authors": [
        {
            "name": "Clayton Liddell",
            "email": "dev@clayliddell.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "~5.0|^6.0",
        "illuminate/database": "~5.0|^6.0",
        "illuminate/events": "~5.0|^6.0",
        "illuminate/support": "~5.0|^6.0",
        "illuminate/validation": "~5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "clayliddell\\ShoppingCart\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "clayliddell\\ShoppingCart\\Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "clayliddell\\ShoppingCart\\ServiceProvider"
            ]
        }
    }
}
